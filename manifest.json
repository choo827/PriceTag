{
  "name": "PriceTag",
  "description": "You can easily convert another currency into your own currency.",
  "version": "0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [ {
    "all_frames": true,
    "css": [ "content_script.css" ],
    "js": [ "content_script.js" ],
    "matches": [ "http://*/*", "https://*/*" ],
    "run_at": "document_end"
  } ],
  "options_page": "options.html",
  "icons": {
    "16": "icon16.ico",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}