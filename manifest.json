{
  "name": "PriceTag",
  "description": "You can easily convert another currency into your own currency.",
  "version": "0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./icon/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [ {
    "all_frames": true,
    "css": [ "content_script.css" ],
    "js": [ "content_script.js" ],
    "matches": [ "http://*/*", "https://*/*" ],
    "run_at": "document_end"
  } ],
  "options_page": "./options/options.html",
  "icons": {
    "16": "./icon/icon16.ico",
    "48": "./icon/icon128.png",
    "128": "./icon/icon128.png"
  }
}